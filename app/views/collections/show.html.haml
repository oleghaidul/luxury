.catalog_items
  =render @collection.items


=content_for :brands do
  Brands
  .vert_menu
    %ul
      -@collection.brand.boutique.brands.each do |b|  
        %li=link_to b.name, [b.boutique, b]

=content_for :collections do
  .catalog_header
    .catalog_name
      %h1 Коллекция 2011
    .collections
      %ul
        -@collections.each do |c|
          %li=link_to "Collection "+c.year.to_s, [c.brand.boutique, c.brand, c]